.nav-bar {
  position: fixed;
  width: 100%;
  background: #fff;
  box-shadow: 0 2px 20px -2px #f0f0f0;
  box-sizing: initial;
  z-index: 100;
}

.search-bar-wrap {
  display: flex;
  align-items: center;
  height: 44px;
}
.back-icon {
  margin-left: 8px;
  width: 20px;
  height: 20px;
}
.search-bar {
  display: flex;
  align-items: center;
  margin-left: 16rpx;
  padding-left: 32rpx;
  width: 468rpx;
  height: 36px;
  border-radius: 18px;
  background: #f5f6f7;
}
.search-icon {
  margin-left: 10px;
  margin-right: 5px;
  width: 16px;
  height: 16px;
}
.search-input {
  padding-left: 10rpx;
  flex: 1;
  height: 30px;
  color: #333;
  font-size: 26rpx;
}
.cancel-btn {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 60rpx;
  height: 60rpx;
}
.cancel-icon {
  width: 32rpx;
  height: 32rpx;
}

.restaurant-list {
  padding: 0 28rpx;
  background: #fff;
}

.no-more-tips {
  margin-top: 36rpx;
  color: #999;
  font-size: 24rpx;
  text-align: center;
}
