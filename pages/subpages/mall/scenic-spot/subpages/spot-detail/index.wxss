.nav-bar {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 44px;
  box-sizing: initial;
  z-index: 100;
}
.nav-bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.bg-cover {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  backdrop-filter: blur(4px);
  z-index: -1;
}
.spot-image {
  width: 30px;
  height: 30px;
  border-radius: 2px;
}
.spot-name {
  margin-left: 8px;
  color: #fff;
  font-size: 16px;
  font-weight: bold;
}

.banner-wrap {
  position: relative;
}
.banner-video {
  position: relative;
}
.banner-dots {
  position: absolute;
  right: 24rpx;
  bottom: 48rpx;
  padding: 0 12rpx;
  height: 40rpx;
  color: #fff;
  font-size: 20rpx;
  line-height: 1;
  font-weight: 500;
  background: rgba(0, 0, 0, 0.3);
  border-radius: 20rpx;
}
.dot-icon {
  margin-right: 8rpx;
  width: 20rpx;
  height: 20rpx;
}
.cur-dot {
  font-size: 24rpx;
}

.video-wrap {
  position: relative;
  width: 100%;
  height: 100%;
}
.video-player {
  width: 100%;
  height: 100%;
}
.mute-icon-wrap {
  position: absolute;
  left: 24rpx;
  bottom: 48rpx;
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  background: rgba(0, 0, 0, 0.3);
}
.mute-icon {
  width: 30rpx;
  height: 30rpx;
}

.container {
  position: absolute;
  left: 0;
  top: 396rpx;
  padding: 20rpx;
  padding-bottom: calc(20rpx + env(safe-area-inset-bottom));
  width: 100%;
  background: #fff;
  border-radius: 24rpx 24rpx 0 0;
}
.name {
  color: #333;
  font-size: 38rpx;
  font-weight: bold;
}
.level-tag {
  margin-left: 8rpx;
  padding: 0 12rpx;
  height: 32rpx;
  color: #5dce86;
  font-size: 20rpx;
  font-weight: bold;
  line-height: 1;
  border-radius: 8rpx;
  background: #d1f7e5;
}
.share-icon {
  width: 42rpx;
  height: 42rpx;
}

.score-wrap {
  display: flex;
  align-items: center;
  margin-top: 10rpx;
}
.star {
  width: 32rpx;
  height: 32rpx;
}
.score {
  margin-left: 10rpx;
  color: #333;
  font-size: 24rpx;
  font-weight: bold;
}

.info-list-wrap {
  margin-top: 8px;
}
.info-list {
  height: 88rpx;
  border-bottom: 1px solid #eee;
}
.info-list:last-child {
  border-bottom: none;
}
.clock-icon, .position-icon {
  margin-right: 12rpx;
  width: 26rpx;
  height: 26rpx;
}
.opening-status {
  margin-right: 12rpx;
  padding: 0 8rpx;
  height: 30rpx;
  color: #37B766;
  font-size: 20rpx;
  font-weight: bold;
  line-height: 1;
  border: 1px solid #37B766;
  border-radius: 4rpx;
}
.opening-status.closed {
  color: #999;
  border: 1px solid #999;
}
.opening-hours {
  color: #333;
  font-size: 24rpx;
  font-weight: bold;
}
.more-detail {
  color: #666;
  font-size: 24rpx;
}
.more-icon {
  margin-left: 2rpx;
  width: 20rpx;
  height: 20rpx;
}

.navigation-btn {
  padding: 0 16rpx;
  height: 36rpx;
  color: #666;
  font-size: 20rpx;
  border-radius: 18rpx;
  background: #f1f1f1;
}
.navigation-icon {
  margin-right: 4rpx;
  width: 18rpx;
  height: 18rpx;
}

.title-wrap {
  margin-top: 48rpx;
}
.title {
  position: relative;
  padding-left: 12rpx;
  color: #333;
  font-size: 30rpx;
  font-weight: bold;
}
.title::before {
  position: absolute;
  top: 50%;
  left: 0;
  transform: translateY(-50%);
  width: 6rpx;
  height: 24rpx;
  content: '';
  background: #f476b4;
  border-radius: 6rpx 0 6rpx 0;
}
.title-tips {
  margin-left: 12rpx;
  color: #529AFE;
  font-size: 24rpx;
}

.ticket-type-menu {
  margin-top: 18rpx;
  white-space: nowrap;
  overflow-x: scroll;
}
.ticket-type-item {
  display: inline-block;
  margin-right: 18rpx;
  padding: 0 18rpx;
  height: 44rpx;
  color: #999;
  font-size: 22rpx;
  line-height: 40rpx;
  border-radius: 22rpx;
  background: #f5f6f7;
  border: 2rpx solid #f5f6f7;
}
.ticket-type-item.active {
  color: #333;
  border: 2rpx solid #ddd;
}

.collapse {
  margin-top: 24rpx;
  background: #f8f8f8;
  border-radius: 18rpx;
}
.collapse-item {
  padding: 0 24rpx;
  border-bottom: 1px solid #eee;
}
.collapse-item:last-child {
  border-bottom: none;
}
.collapse-trunk {
  height: 88rpx;
}
.collapse-trunk-title {
  margin-right: 8rpx;
  flex: 1;
  color: #333;
  font-size: 28rpx;
  font-weight: bold;
}
.collapse-trunk-content {
  color: #666;
  font-size: 22rpx;
}
.base-price {
  color: #EE0D23;
}
.arrow {
  margin-left: 4rpx;
  width: 24rpx;
  height: 24rpx;
}
.collapse-subitem {
  padding: 24rpx 0;
  border-bottom: 1px solid #eee;
}
.collapse-subitem:last-child {
  border-bottom: none;
}

.shop-type {
  margin-right: 8rpx;
  padding: 0 8rpx;
  height: 32rpx;
  color: #FFE5BD;
  font-size: 22rpx;
  line-height: 1;
  background: linear-gradient(270deg, #404A5C 0%, #0F131A 100%);
  border-radius: 6rpx;
}
.shop-name {
  color: #333;
  font-size: 26rpx;
  font-weight: bold;
}
.tips-wrap {
  margin-top: 18rpx;
}
.tips {
  padding-right: 12rpx;
  border-right: 1px solid #ddd;
  margin-right: 12rpx;
  color: #666;
  font-size: 22rpx;
  line-height: 1;
}
.tips:last-child {
  border-right: none;
}
.tips.stress {
  color: #529AFE;
}
.booking-wrap {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.booking-btn {
  margin-top: 8rpx;
  width: 100rpx;
  height: 60rpx;
  color: #fff;
  font-size: 24rpx;
  border-radius: 30rpx;
  background: #1986F8;
}

.comment-item {
  margin-top: 24rpx;
  padding: 24rpx;
  background: #fff;
  box-shadow: 0 0 12rpx 6rpx #f5f6f7;
  border-radius: 18rpx;
}
.user-avatar {
  width: 68rpx;
  height: 68rpx;
  border-radius: 50%;
}
.user-info {
  margin-left: 18rpx;
  flex: 1;
}
.user-name {
  color: #333;
  font-size: 24rpx;
}
.comment-data {
  color: #999;
  font-size: 20rpx;
}
.comment-rate {
  margin-left: 8rpx;
  color: #333;
  font-size: 24rpx;
  font-weight: bold;
}
.comment-content {
  margin-top: 18rpx;
  color: #333;
  font-size: 24rpx;
}
.comment-images {
  position: relative;
  margin-top: 18rpx;
  border-radius: 8rpx;
  overflow: hidden;
}
.comment-image {
  width: 214rpx;
  height: 214rpx;
  border-radius: 8rpx;
}
.more-image-tips {
  position: absolute;
  right: 0;
  bottom: 0;
  width: 50rpx;
  height: 50rpx;
  color: #333;
  font-size: 22rpx;
  background: rgba(255, 255, 255, 0.6);
  border-radius: 4rpx;
}

.total-tips {
  margin-top: 24rpx;
  color: #666;
  font-size: 24rpx;
}

.qa-list {
  margin-top: 24rpx;
}
.qa-item {
  position: relative;
  padding: 24rpx 42rpx;
  border-bottom: 1px solid #eee;
}
.qa-item:first-child {
  margin-bottom: 8px;
  background: #fff;
  box-shadow: 0 0 12rpx 6rpx #f5f6f7;
  border-radius: 18rpx;
  border-bottom: none;
}
.qa-item:last-child {
  margin-bottom: 24rpx;
}
.qa-question {
  color: #333;
  font-size: 24rpx;
  font-weight: bold;
}
.answer-user {
  margin-top: 18rpx;
  margin-left: 8rpx;
}
.qa-answers-avatars {
  margin-left: 8rpx;
}
.answer-user-avatar {
  margin-left: -8rpx;
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  border: 2rpx solid #fff;
}
.answer-user-name {
  margin-left: 8rpx;
  color: #333;
  font-size: 22rpx;
}
.qa-answers {
  margin-top: 12rpx;
}
.qa-answer {
  margin-top: 4rpx;
  color: #666;
  font-size: 22rpx;
}
.qa-answers-number {
  margin-left: 8rpx;
  color: #666;
  font-size: 22rpx;
}
.qa-label {
  position: absolute;
  left: 0;
  top: 26rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 30rpx;
  height: 30rpx;
  color: #fff;
  font-size: 20rpx;
  font-weight: bold;
  line-height: 1;
  background: #1986F8;
  border-radius: 0 6rpx 6rpx 0;
}

.hotel-list {
  margin-top: 24rpx;
  white-space: nowrap;
  overflow-x: scroll;
}
