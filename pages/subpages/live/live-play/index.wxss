.rooms-wrap {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background: #000;
}
.bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  -webkit-filter: blur(20px);
  -moz-filter: blur(20px);
  -ms-filter: blur(20px);    
  filter: blur(20px);
  transform: scale(1.2);
}
.back-icon{
  position: fixed;
  left: 0;
  display: flex;
  align-items: center;
  padding-left: 8px;
  height: 44px;
}
