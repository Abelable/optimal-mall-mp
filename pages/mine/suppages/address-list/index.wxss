.container {
  margin: 24rpx 24rpx calc(204rpx + env(safe-area-inset-bottom));
  border-radius: 24rpx;
  background: #fff;
}

.address-list-wrap {
  padding-bottom: 200rpx;
}
.address-list-inner {
  display: flex;
  align-items: center;
  height: 150rpx;
  padding: 0 24rpx;
  width: 100%;
}
.address-content {
  flex: 1;
  color: #999;
  font-size: 26rpx;
}
.user-info {
  display: flex;
  align-items: center;
  color: #333;
  font-size: 28rpx;
}
.user-name {
  margin-right: 20rpx; 
}
.default-icon {
  margin-left: 36rpx;
  padding: 2rpx 6rpx;
  font-size: 20rpx;
  color: #B87900;
  border-radius: 8rpx;
  border: 2rpx solid #000;
}
.address-detail {
  margin-top: 16rpx;
}

.swipe-cell-delete-btn {
  width: 65px;
  height: 100%;
  color: #fff;
  font-size: 28rpx;
  background: #FD0807;
}

.add-btn {
  position: fixed;
  left: 50%;
  bottom: calc(100rpx + env(safe-area-inset-bottom));
  transform: translateX(-50%);
  width: 580rpx;
  height: 80rpx;
  color: #fff;
  font-size: 28rpx;
  border-radius: 40rpx;
  background: #212121;
}
