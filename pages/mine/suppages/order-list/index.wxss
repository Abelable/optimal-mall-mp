.menu {
  display: flex;
  align-items: center;
  width: 100%;
  height: 40px;
  background: #fff;
  border-top: 1px solid #f5f6f8;
}
.menu-item {
  flex: 1;
  text-align: center;
  font-size: 14px;
  color: #666;
}
.menu-item.selected {
  font-size: 15px;
  color: #333;
  font-weight: 600;
}

.content {
  margin-top: 40px;
  padding: 24rpx;
}

.no-more-tips {
  margin-top: 36rpx;
  color: #999;
  font-size: 24rpx;
  text-align: center;
}
