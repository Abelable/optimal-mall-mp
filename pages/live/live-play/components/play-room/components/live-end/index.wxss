.live-end-wrap {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .5);
  color: #fff;
}
.back-icon-wrap {
  position: absolute;
  left: 0;
  display: flex;
  align-items: center;
  padding-left: 8px;
  height: 44px;
}
.anchor-info {
  margin-top: 160rpx;
  text-align: center;
}
.anchor-avatar {
  width: 176rpx;
  height: 176rpx;
  border-radius: 50%;
  border: 4rpx solid #fff;
}
.anchor-name {
  font-size: 15px;
  line-height: 21px;
  margin-top: 10px;
}
.time-wrap {
  margin-top: 68rpx;
  margin-left: 10%;
  width: 80%;
  text-align: center;
}
.line {
  width: 100%;
  height: 2rpx;
  background: linear-gradient(90deg,rgba(238,238,238,0) 0%,rgba(255,255,255,1) 49%,rgba(216,216,216,0) 100%);
  opacity: 0.4;
}
.tips {
  margin-top: 20rpx;
  font-size: 40rpx;
}
.time {
  margin-bottom: 20rpx;
  font-size: 24rpx;
  color: #B9B9B9;
}
.harvest-wrap {
  margin-top: 70rpx;
  margin-left: 20%;
  width: 60%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.watcher-num, .parise-num {
  font-size: 24rpx;
  text-align: center;
  color: #B9B9B9;
}
.num {
  font-size: 30rpx;
}